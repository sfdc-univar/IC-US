    <validationRules>
        <fullName>ActiveCannotBelongToAdminId</fullName>
        <active>true</active>
        <description>If changing back to Active, they have to assign to a real user</description>
        <errorConditionFormula>AND( ISPICKVAL(Contact_Status__c, &quot;Active&quot;) ,  OwnerId = &apos;00530000000dTej&apos;, NOT(ISCHANGED(OwnerId)))</errorConditionFormula>
        <errorMessage>Must assign to a real person if reactivating this contact.
Press “[Change]” next to the Contact Owner field to change the owner, then Save, and contact will automatically be reactivated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BTQQuoteContactOnShipToAcctOnly</fullName>
        <active>true</active>
        <description>Contacts set up as a Price Quote Email contact must be on Ship-To accounts (not prospects etc)</description>
        <errorConditionFormula>AND(Account.CustomerNumber__c  = &quot;&quot;,EmailQuoteBillTo__c = true)</errorConditionFormula>
        <errorDisplayField>EmailQuoteBillTo__c</errorDisplayField>
        <errorMessage>Contact&apos;s Account Record Type must be Ship-To to receive quote emails.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BTQQuoteContactRequiresEmailAddress</fullName>
        <active>true</active>
        <description>Contacts set up as a Price Quote Email contact must have an email address</description>
        <errorConditionFormula>AND( Email   = &quot;&quot;, EmailQuoteBillTo__c = true,NOT(CONTAINS($Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Email address required for a contact to receive prices quotes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactInvalidCharacters</fullName>
        <active>true</active>
        <description>RegEx based on ExactTarget email validation rules</description>
        <errorConditionFormula>AND(NOT(Regex( Email , &quot;^[a-z0-9!#$%&amp;&apos;*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&amp;&apos;*+/=?^_`{|}~-]+)*@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.)|(([a-zA-Z0-9_\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})$&quot;)),NOT(CONTAINS($Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Invalid Email Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EmailCannotContainUnivarusaDotCom</fullName>
        <active>true</active>
        <description>Email Addresses can&apos;t contain &quot;Univarusa.com&quot; as it can create problems for the Exact Target Email Quotes Job. Exception for SysAdmins on their own contacts, so we can set up test data as needed.</description>
        <errorConditionFormula>AND(CONTAINS( Email , &quot;univarusa.com&quot;),  NOT(CONTAINS( $Profile.Name,&quot;System Admin&quot;)))</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>&quot;Univarusa.com&quot; email addresses are not allowed on Contacts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FalseFlagMustBeFalse</fullName>
        <active>true</active>
        <description>prevents this from ever being set to true</description>
        <errorConditionFormula>FalseFlag__c  = True</errorConditionFormula>
        <errorDisplayField>FalseFlag__c</errorDisplayField>
        <errorMessage>False Flag value must always be False</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InactiveCannotBelongToRealPerson</fullName>
        <active>true</active>
        <description>If inactive, can&apos;t belong to a real user.  This only happens when they create a new contact and immediately set it to Inactive before saving the first time.</description>
        <errorConditionFormula>AND( ISNEW() , ISPICKVAL(Contact_Status__c, &quot;Inactive&quot;) )</errorConditionFormula>
        <errorDisplayField>Contact_Status__c</errorDisplayField>
        <errorMessage>Cannot save new contact as Inactive.  To inactivate a contact, first save as Active, then go back in and edit it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoEmailQuotesForInactiveAccounts</fullName>
        <active>true</active>
        <description>If we allow quotes to be emailed to inactive UVX customers, we run the risk of deleting those accounts in the monthly account cleanup.</description>
        <errorConditionFormula>AND( Account.CustomerStatus__c  = &quot;INACTIVE&quot;, EmailQuoteBillTo__c  = true, NOT(CONTAINS($Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorDisplayField>EmailQuoteBillTo__c</errorDisplayField>
        <errorMessage>This contact&apos;s Account is inactive in UVX - cannot email quotes.  Please reassign Contact to an active account before setting quote flag.</errorMessage>
    </validationRules>
