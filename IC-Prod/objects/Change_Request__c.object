    <validationRules>
        <fullName>Cancellation_Reason</fullName>
        <active>true</active>
        <description>If status is cancelled, then they need to enter a reason.</description>
        <errorConditionFormula>AND( Cancellation_Reason__c  = &quot;&quot;, ISPICKVAL( Change_Request_Status__c , &quot;Cancelled&quot;))</errorConditionFormula>
        <errorDisplayField>Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please enter the reason you are cancelling this Change Request.  If you did not intend to cancel this request, then update the status to a different value and remove the cancellation reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Requires_CHG</fullName>
        <active>true</active>
        <description>All completed CRs require a Remedy CHG # to be entered.</description>
        <errorConditionFormula>AND(ISPICKVAL( Change_Request_Status__c , &quot;Completed&quot;),  TRIM(Remedy_CRQ__c)  = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Remedy_CRQ__c</errorDisplayField>
        <errorMessage>A Remedy CHG # is required on all Completed changes.  If you have not already created the Remedy CHG ticket for this change, please do so, then come back and enter the CHG number here.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Requires_Date</fullName>
        <active>true</active>
        <description>All completed CRs require Actual Completion Date</description>
        <errorConditionFormula>AND(ISPICKVAL( Change_Request_Status__c , &quot;Completed&quot;),   ISBLANK(Actual_Completion_Date__c ))</errorConditionFormula>
        <errorDisplayField>Actual_Completion_Date__c</errorDisplayField>
        <errorMessage>Actual Completion Date is required on all Completed requests.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_and_Deployed</fullName>
        <active>true</active>
        <description>If status is completed, then deployment date must be entered - and vice versa</description>
        <errorConditionFormula>AND( NOT( ISNULL( Deployment__c )),NOT(ISPICKVAL( Change_Request_Status__c , &quot;Completed&quot;)))</errorConditionFormula>
        <errorDisplayField>Change_Request_Status__c</errorDisplayField>
        <errorMessage>If Deployment Date has been entered, then status must be Completed.  If this Change Request is not actually completed, please do not enter a deployment date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Assigned_To</fullName>
        <active>true</active>
        <description>Require an assignee if the status is &apos;Assigned&apos; or &apos; In Development&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL(Assigned_To__c, &quot;&quot;), OR(ISPICKVAL(Change_Request_Status__c , &quot;Assigned&quot;),ISPICKVAL(Change_Request_Status__c , &quot;In Development&quot;)))</errorConditionFormula>
        <errorDisplayField>Assigned_To__c</errorDisplayField>
        <errorMessage>Change Request must be assigned if status is &apos;Assigned&apos; or &apos;In Development&apos;</errorMessage>
    </validationRules>
