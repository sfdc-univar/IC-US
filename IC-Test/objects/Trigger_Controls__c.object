<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Used to turn on / off triggers in SFDC</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Check_Duplicate_Alias__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Whether or not to check duplicate aliases when a User record is saved</description>
        <externalId>false</externalId>
        <label>Check for Duplicate User Aliases</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Giam_Trigger__c</fullName>
        <defaultValue>false</defaultValue>
        <description>If Checked, Trigger is active.</description>
        <externalId>false</externalId>
        <inlineHelpText>Uncheck the box to deactivate the Giam User trigger.  Check the box to activate it.</inlineHelpText>
        <label>Giam Trigger</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>User_Last_Modified_Field__c</fullName>
        <defaultValue>true</defaultValue>
        <description>If this is checked, all the triggers that have to do with User Last Modified User and Date, will fire. Uncheck and they will now. (Accounts, Contacts, Opportunities, Waste Streams, CPPs, Sample Requests and Activites)</description>
        <externalId>false</externalId>
        <label>User Last Modified Field</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>User_s_Manager__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Sets User&apos;s Manager based on the Manager PerNr on the User record.</description>
        <externalId>false</externalId>
        <label>User&apos;s Manager</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>CustomTriggerControls</label>
</CustomObject>
