CASE(Organization_wide_discontent__c, &quot;Yes&quot;, +4, &quot;No&quot;, -4, &quot;Unknown&quot;, +0, 0) +
CASE(Current_supplier_falls_short__c, &quot;Yes&quot;, +4, &quot;No&quot;, -4, &quot;Unknown&quot;, +0, 0) +
