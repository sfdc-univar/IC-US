CASE(General_organizational_needs_unmet__c, &quot;Yes&quot;, +4, &quot;No&quot;, -4, &quot;Unknown&quot;, +0, 0) +
CASE(Frequent_sharing_of_best_practices__c, &quot;Yes&quot;, +6, &quot;No&quot;, -6, &quot;Unknown&quot;, +0, 0) +
CASE(Multiple_stakeholders_learning__c, &quot;Yes&quot;, +6, &quot;No&quot;, -6, &quot;Unknown&quot;, +0, 0) +
CASE(Senior_leaders_reach_out_for_ideas__c, &quot;Yes&quot;, +6, &quot;No&quot;, -6, &quot;Unknown&quot;, +0, 0) +
CASE(Improvement_initiatives_as_ongoing__c, &quot;Yes&quot;, +6, &quot;No&quot;, -6, &quot;Unknown&quot;, +0, 0) +
CASE(Engages_in_constructive_dialogue__c, &quot;Yes&quot;, +10, &quot;No&quot;, -10, &quot;Unknown&quot;, +0, 0) +
CASE(Articulate_differences_current_vs_new__c, &quot;Yes&quot;, +10, &quot;No&quot;, -10, &quot;Unknown&quot;, +0, 0) +
CASE(Request_continuing_conversations__c, &quot;Yes&quot;, +10, &quot;No&quot;, -10, &quot;Unknown&quot;, +0, 0) +
CASE(Express_openness_to_escalation__c, &quot;Yes&quot;, +10, &quot;No&quot;, -10, &quot;Unknown&quot;, +0, 0) +
