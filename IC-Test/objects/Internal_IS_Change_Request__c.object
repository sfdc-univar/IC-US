    <listViews>
        <fullName>WaitingforApproval</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>Assigned_To__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>notContain</operation>
            <value>Cancelled,Completed</value>
        </filters>
        <filters>
            <field>Final_Approval_Date__c</field>
            <operation>equals</operation>
        </filters>
        <label>Waiting for Approval</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <label>Internal IS Change Request Name</label>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Internal IS Change Requests</pluralLabel>
    <searchLayouts/>
    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Completed_Requires_CHG</fullName>
        <active>true</active>
        <description>All completed ICRs require a Remedy CHG # to be entered.</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Completed&quot;),  TRIM(Remedy_CRQ__c)  = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Remedy_CRQ__c</errorDisplayField>
        <errorMessage>A Remedy CHG # is required on all Completed changes.  If you have not already created the Remedy CHG ticket for this change, please do so, then come back and enter the CHG number here.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Requires_Date</fullName>
        <active>true</active>
        <description>Need to enter Actual Completion Date, if completed</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Completed&quot;),   TEXT(Actual_Completion_Date__c)  = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Actual_Completion_Date__c</errorDisplayField>
        <errorMessage>Actual Completion Date is required if you are Completing a ticket.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Assigned_To</fullName>
        <active>true</active>
        <description>Require an assignee if the status is &apos;Assigned&apos; or &apos; In Development&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL(Assigned_To__c, &quot;&quot;), OR(ISPICKVAL(Status__c , &quot;Assigned&quot;),ISPICKVAL(Status__c , &quot;In Development&quot;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Change Request must be assigned if status is &apos;Assigned&apos; or &apos;In Development&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Final_Approval_Date</fullName>
        <active>true</active>
        <description>We can&apos;t let CRs slip by w/o saying when they were approved</description>
        <errorConditionFormula>AND(NOT( ISPICKVAL(Assigned_To__c,  &quot;&quot;)), TEXT( Final_Approval_Date__c) = &quot;&quot;,NOT(ISPICKVAL( Status__c ,  &quot;Cancelled&quot;)),NOT(ISPICKVAL( Status__c ,  &quot;Open&quot;)))</errorConditionFormula>
        <errorDisplayField>Final_Approval_Date__c</errorDisplayField>
        <errorMessage>Enter Final Approval Date!</errorMessage>
    </validationRules>
</CustomObject>
